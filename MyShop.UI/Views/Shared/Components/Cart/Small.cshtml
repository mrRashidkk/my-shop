@model IEnumerable<MyShop.Application.Cart.GetCart.Response>

@{

}

<div>
    <h2>Small cart component</h2>
    @foreach (var product in Model)
    {
        <div>
            <span>@product.Name -</span>
            <span>@product.Value -</span>
            <span>@product.Qty -</span>
        </div>
    }
</div>